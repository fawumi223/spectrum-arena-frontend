{
  "deploy": {
    "builder": "NODE",
    "static": {
      "src": "./build",
      "headers": [
        {
          "source": "**/*.js",
          "headers": { "Cache-Control": "public, max-age=31536000, immutable" }
        }
      ]
    },
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "startCommand": null
  }
}

